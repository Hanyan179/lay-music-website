{
  "name": "fate-echoes",
  "version": "0.1.0",
  "description": "宿命回响 - Fate Echoes",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "sync-assets": "node sync-assets.js",
    "dev:sync": "npm run sync-assets && npm run dev",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "image-server": "node server.js"
  },
  "dependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jquery": "^3.7.1",
    "lottie-web": "^5.13.0",
    "multer": "^2.0.1",
    "pinia": "^2.1.7",
    "tailwindcss": "^3.4.16",
    "turn.js": "^1.0.5",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "vue3-lottie": "^3.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/compiler-sfc": "^3.3.4",
    "autoprefixer": "^10.4.21",
    "fs-extra": "^11.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.9"
  }
